{
  "name": "proxy",
  "private": true,
  "scripts": {
    "up": "docker compose up -d && pnpm run migrate && pnpm run build && pm2 start",
    "migrate": "pnpm run up-db && turbo run migrate",
    "type-check": "turbo run type-check",
    "up-db": "docker compose up --build -d postgres-nina",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@prisma/client": "7.0.1",
    "@prisma/client-runtime-utils": "^7.0.1",
    "dotenv": "^17.2.3"
  }
}
