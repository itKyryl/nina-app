{
  "name": "@repo/core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "build": "tsc && rm -rf dist/clients/prisma/generated && cp -r src/clients/prisma/generated dist/clients/prisma/generated",
    "generate": "dotenv -e ./../../.env prisma generate",
    "migrate": "dotenv -e ./../../.env prisma migrate dev && pnpm run generate",
    "seed": "dotenv -e ./../../.env ts-node ./src/clients/prisma/seed/index.ts"
  },
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    }
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.34.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/big.js": "^6.2.2",
    "@types/lodash": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "axios": "^1.13.2",
    "big.js": "^7.0.1",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "piscina": "^5.1.4",
    "prisma": "^7.0.1",
    "zod": "^4.1.12"
  }
}
