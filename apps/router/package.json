{
  "name": "router",
  "version": "1.0.0",
  "description": "router",
  "private": true,
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "start": "node dist/main.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "dev": "pnpm --filter @repo/core build && cross-env PORT=6010 dotenv -e ./../../.env -- nodemon --inspect=0.0.0.0:8329 --signal SIGINT --nolazy src/main.ts",
    "prod-dev": "pnpm --filter @repo/core build && cross-env PORT=6010 dotenv -e ./.jest.env -- nodemon --inspect=0.0.0.0:8329 --signal SIGINT --nolazy src/main.ts"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.12",
    "@types/papaparse": "^5.3.16",
    "@types/pdf-parse": "^1.1.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "latest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@nestjs/common": "^11.1.1",
    "@nestjs/core": "^11.1.1",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@repo/core": "workspace:*",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "big.js": "^7.0.1",
    "csv-parse": "^5.6.0",
    "express": "^4.19.2",
    "moment-timezone": "^0.5.46",
    "papaparse": "^5.5.2",
    "swagger-ui-express": "^5.0.1",
    "pdf-parse": "^1.1.1",
    "zod": "3.24.4"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node --require ts-node/register"
    }
  },
  "packageManager": "pnpm@9.0.0"
}
